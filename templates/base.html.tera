<!DOCTYPE html>
<html lang="en">

<head>
    <title>{% block title %} BGPHijacks Dashboard {% endblock title %}</title>

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="/app/images/favicon.ico" type="image/ico"/>

    <!-- Bootstrap -->
    <link href="/app/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="/app/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="/app/vendors/datatables/jquery.dataTables.min.css" rel="stylesheet">
    <!-- bootstrap-progressbar -->
    <link href="/app/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
    <!-- bootstrap-daterangepicker -->
    <link href="/app/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">
    <link href="/app/vendors/leaflet/leaflet.css" rel="stylesheet">
    <!-- Custom Theme Style -->
    <link href="/app/build/css/custom.min.css" rel="stylesheet">
    <link href="/app/custom/custom.css" rel="stylesheet">

</head>

<body class="nav-md" onload="{{context.onload_function}}">

<div class=" container body">
    <div class="main_container">
        <div class="col-md-3 left_col">
            <div class="left_col scroll-view">
                <div class="navbar nav_title" style="border: 0;">
                    <a href="/events/all" class="site_title"><i class="fa fa-paw"></i> <span>BGPHijacks Dashboard</span></a>
                </div>

                <div class="clearfix"></div>

                <!-- sidebar menu -->
                <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                    <div class="menu_section">
                        <!--<h3>General</h3>-->
                        <ul class="nav side-menu">
                            <li><a href="/events/all"><i class="fa fa-home"></i> Home </a></li>
                            <li><a><i class="fa fa-table"></i> Events <span class="fa fa-chevron-down"></span></a>
                                <ul class="nav child_menu">
                                    <li><a href="/events/moas">Moas</a></li>
                                    <li><a href="/events/submoas">Submoas</a></li>
                                    <li><a href="/events/defcon">Defcon</a></li>
                                    <li><a href="/events/edges">Edges</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
                <!-- /sidebar menu -->
            </div>
        </div>

        <!-- page content -->
        <div class="right_col" role="main">

            {% block content %}
            {% endblock content %}

        </div>
        <!-- /page content -->

        <!-- footer content -->
        <footer>
            <div class="pull-right">
                CAIDA HIJACKS Project
            </div>
        </footer>
        <!-- /footer content -->
    </div>
</div>

<!-- jQuery -->
<script src="/app/vendors/jquery/dist/jquery.min.js"></script>
<!-- Bootstrap -->
<script src="/app/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
<!-- Chart.js -->
<!--<script src="/app/vendors/Chart.js/dist/Chart.min.js"></script>-->
<!-- Flot -->
<script src="/app/vendors/Flot/jquery.flot.js"></script>
<script src="/app/vendors/Flot/jquery.flot.pie.js"></script>
<script src="/app/vendors/Flot/jquery.flot.time.js"></script>
<script src="/app/vendors/Flot/jquery.flot.stack.js"></script>
<script src="/app/vendors/Flot/jquery.flot.resize.js"></script>
<!-- Flot plugins -->
<script src="/app/vendors/flot.orderbars/js/jquery.flot.orderBars.js"></script>
<script src="/app/vendors/flot-spline/js/jquery.flot.spline.min.js"></script>
<script src="/app/vendors/flot.curvedlines/curvedLines.js"></script>
<!-- DateJS -->
<script src="/app/vendors/DateJS/build/date.js"></script>

<script src="/app/vendors/GoogleCharts/loader.js"></script>
<!-- bootstrap-daterangepicker -->
<script src="/app/vendors/moment/min/moment.min.js"></script>
<script src="/app/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>

<!-- datatables -->
<script src="/app/vendors/datatables/jquery.dataTables.min.js"></script>

<!--leaflet-->
<script src="/app/vendors/leaflet/leaflet.js"></script>
<script src="/app/vendors/leaflet/leaflet.textpath.js"></script>

<!-- Custom Scripts -->
<script src="/app/custom/load_data.js"></script>
<script src="/app/custom/datatable.js"></script>
<script src="/app/custom/event_rendering.js"></script>
<script src="/app/custom/traceroutes.js"></script>
<script src="/app/custom/utils.js"></script>
<script src="/app/custom/custom.js"></script>

</body>

</html>