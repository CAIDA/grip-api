{% extends "base" %}

{% block content %}

<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2 id="table_title"> Traceroutes </h2>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                <table id="traceroutes_table" class="table table-striped table-bordered" style="max-width:95%;">
                </table>
            </div>
        </div>
    </div>
</div>

<br />

<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <ul class="nav navbar-right panel_toolbox">
                    <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                    </li>
                </ul>
